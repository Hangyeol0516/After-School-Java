import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("출력할 단 입력");
		int a = scan.nextInt();
		
		System.out.println("몇까지 곱할지 입력");
		int b = scan.nextInt();
		
		for (int i=1; i<=b; i++) {
			System.out.println(a+"*"+i+"="+a*i);
		}
		
	}

}
