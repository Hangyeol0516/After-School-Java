package main;

public class Main {

	public static void main(String[] args) {

		int result = 0;
		
		for (int i = 1; i <= 40; i++) {
		
			int num = i;
			
			while (true) {
				if (num%10==3 || num%10==6 || num%10==9) {
					result += 1;
				}
				num = num / 10;
				if (num == 0) {
					break;
				}
			}
		}
		System.out.println(result);
	}

}
