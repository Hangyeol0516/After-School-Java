package main;

public class Main {

	public static void main(String[] args) {

		int result = 0;
		
		for (int i = 1; i <= 40; i++) {
			
			if (i%10==3 || i%10==6 || i%10==9) {
				result += 1;
			}
			
			if (i/10==3 || i/10==6 || i/10==9) {
				result += 1;
			}
		}
		System.out.println(result);
	}

}
