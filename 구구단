package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		
		System.out.println("몇 단까지");
		int n = scan.nextInt();
		System.out.println("몇 곱까지");
		int x = scan.nextInt();
		
		for (int i = 2; i <= n; i++) {
			for (int j = 1; j <= x; j++) {
				System.out.println(i+" * "+j+"= "+i*j);
			}
			System.out.println();
		}
	}

}
