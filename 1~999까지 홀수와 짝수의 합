package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		
		int even = 0;
		int odd = 0;
		
		for (int i = 1; i <= 999; i++) {
			if (i%2==0) {
				even += i;
			} else {
				odd += i;
			}
		}
		System.out.println("짝수의 합 : " + even + " 홀수의 합 : " + odd);
	}

}
